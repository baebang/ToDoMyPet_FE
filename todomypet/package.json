{
  "name": "todomypet",
  "version": "0.0.1",
  "private": true,
  "plugins": [
    [
      "react-native-permissions",
      {
        "iosPermissions": [
          "Notifications",
          "Camera",
          "PhotoLibrary",
          "PhotoLibraryAddOnly"
        ]
      }
    ]
  ],
  "scripts": {
    "android-d": "react-native run-android --variant=debug",
    "android": "react-native run-android --variant=release",
    "ios": "react-native run-ios ",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build": "react-native run-ios && react-native run-android",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-clipboard/clipboard": "^1.13.1",
    "@react-native-firebase/app": "^19.0.0",
    "@react-native-firebase/auth": "^19.0.0",
    "@react-native-firebase/firestore": "^19.0.0",
    "@react-native-firebase/messaging": "^19.0.0",
    "@react-native-firebase/storage": "^19.0.0",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "add": "^2.0.6",
    "axios": "^1.6.2",
    "date-fns": "^3.3.1",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-bouncy-checkbox": "^3.0.7",
    "react-native-calendars": "^1.1303.0",
    "react-native-confetti": "^0.1.0",
    "react-native-date-picker": "^4.3.5",
    "react-native-easy-toast": "^2.3.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-hyperlink": "^0.0.22",
    "react-native-image-picker": "^7.1.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-pager-view": "^6.2.3",
    "react-native-permissions": "^4.1.4",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "^4.8.1",
    "react-native-screens": "^3.29.0",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-splash-screen": "^3.3.0",
    "react-native-web-swiper": "^2.2.4",
    "react-query": "^3.39.3",
    "zustand": "^4.4.7"
  },
  "reactNativePermissionsIOS": [
    "Camera",
    "Notifications",
    "PhotoLibrary",
    "PhotoLibraryAddOnly"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
